		<Element Type="SqlProcedure" Name="[dbo].[UpdateClientAccount]">
			<Property Name="BodyScript">
				<Value><![CDATA[
BEGIN
    SET NOCOUNT ON;
        UPDATE [dbo].[Client]
        SET Firstname = @Firstname,
            Lastname = @Lastname,
            MobileNumber = @MobileNumber,
            Gender = @Gender,
            Address = @Address,
            Birthdate = @Birthdate
        WHERE Id = @ClientId;

        UPDATE [dbo].[Account]
        SET Balance = ISNULL(@Balance, 0.00),
            ModifiedDate = GETDATE()
        WHERE ClientId = @ClientId;
    
END;]]></Value>
